{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {},
    "env": {
        "es6": true
    },
    "overrides": [
        {
            "files": ["*"],
            "rules": {
                "@typescript-eslint/no-explicit-any": "off"
            }
        }
    ]
}